<Window x:Class="bazagpr.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:bazagpr"
        mc:Ignorable="d"
        Title="Baza GPR" Height="450" Width="800" Loaded="Window_Loaded" Closed="Window_Closed" WindowStartupLocation="CenterScreen">
    <Grid>
        <DataGrid x:Name ="GPRDataGrid"  HorizontalAlignment="Left" Height="260"
                  Margin="10,67,0,0" VerticalAlignment="Top" Width="500"/>
        <Button x:Name="refresh_btn" Content="Odśwież" HorizontalAlignment="Left"
                Margin="20,20,0,0" VerticalAlignment="Top" Width="75"
                RenderTransformOrigin="-0.267,1.6" Click="Refresh_btn_Click"/>
    </Grid>
</Window>
